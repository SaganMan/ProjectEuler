# Project Euler Problem 4
# Largest Palindrome from product of two 3-digit numbers

def palin(n,f,p):
    s=str(n)
    l=len(s)-1
    i=0
    while (i < len(s)) and (l >= 0):
        if s[i]!=s[l]:
            f=0
        i=i+1
        l=l-1

    if f==1:
        p.append(n)

n=0
f=1
m=99999
p=[]

while m>=10000:
    for i in range(10000,(m+1)):
        n=i*m
        palin(n,f,p)
    m=m-1

print(max(p))

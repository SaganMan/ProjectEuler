# Project Euler Problem 3
# Largest Prime Factor of 600851475143

def SieveOfEratosthenes(n,prime):
    p = 2
    while (p * p <= n):
        if (prime[p] == True):
            for i in range(p * 2, n + 1, p):
                prime[i] = False
        p += 1

    for p in range(2, n):
        if prime[p]:
            pr.append(p)

t=600851475143
pr=[]
n=90000000
factr=[]
prime = [True for i in range(n + 1)]
SieveOfEratosthenes(n,prime)
for i in range(0,len(pr)):
    if not t%pr[i]:
        factr.append(pr[i])
print(factr)
f1=[]

for i in range(0,len(factr)):
    h=int(t/factr[i])
    f1.append(h)
    if h<n:
        if prime[h]:
            print(h,"is prime")
        else:
            print(h,"is not prime")
f2=[]
print(f1)

for a in range(0,len(f1)):
    for b in range(0, len(pr)):
        if not f1[a]%pr[b]:
            f2.append(pr[b])
print(f2)
